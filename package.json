{
  "name": "aws-serverless-training",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "outputs": "aws cloudformation describe-stacks --stack-name dev-ServerlessTrainingPipelineStack | jq '.Stacks | .[] | .Outputs | reduce .[] as $i ({}; .[$i.OutputKey] = $i.OutputValue)'",
    "postinstall": "husky install && check-node-version --package"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.91",
    "@swc/core": "1.2.223",
    "@swc/jest": "0.2.22",
    "@types/aws-lambda": "8.10.101",
    "@types/backoff": "^2.5.2",
    "@types/jest": "28.1.3",
    "@types/node": "18.0.0",
    "@types/prettier": "2.6.3",
    "@types/sharp": "0.30.4",
    "aws-cdk": "2.29.0",
    "backoff": "2.5.0",
    "check-node-version": "4.2.1",
    "esbuild": "0.14.47",
    "id128": "1.6.6",
    "jest": "28.1.1",
    "node-fetch": "3.2.10",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.113.0",
    "@aws-sdk/client-rekognition": "3.112.0",
    "@aws-sdk/client-s3": "3.117.0",
    "@nordicsemiconductor/from-env": "1.0.1",
    "aws-cdk-lib": "2.29.0",
    "constructs": "10.1.42",
    "source-map-support": "0.5.21",
    "tsx": "3.8.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
