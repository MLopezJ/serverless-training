    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.100.0",
    "@aws-sdk/client-rekognition": "^3.110.0",
    "@aws-sdk/client-s3": "^3.105.0",
    "aws-cdk-lib": "2.26.0",
